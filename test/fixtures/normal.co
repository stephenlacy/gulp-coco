export class Cat extends Animal
  (@name) ->
    super ...
    @meowed = 0
    @meow   = ~> ++@meowed
  @interval = 5000ms
  play: -> setInterval @meow, ..interval
  bark: -> ...

switch day
case \Fri \Sat
  if day is not rainy
    go bingo
    go dancing
case \Sun then go church
default go work
